{
    "name": "temhri/yii2-datatables",
    "type": "yii2-extension",
    "description": "DataTables widget for Yii2",
    "keywords": [
        "yii2",
        "extension",
        "datatables"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/temhri/yii2-datatables/issues?state=open",
        "source": "https://github.com/temhri/yii2-datatables"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "^2.0",
        "bower-asset/datatables": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "rector/rector": "^0.18.2"
    },
    "autoload": {
        "psr-4": {
            "temhri\\datatables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "temhri\\datatables\\tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
